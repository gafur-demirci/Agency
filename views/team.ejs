<%- include('partials/_header') %>

    <body id="page-top">

        <!-- Navigation-->
        <%- include('partials/_navbar') %>

        <!-- Masthead-->
        <header class="masthead">
            <div class="container">
                <div class="masthead-subheading">Welcome To Our Studio!</div>
                <div class="masthead-heading text-uppercase">It's Nice To Meet You</div>
                <a class="btn btn-primary btn-xl text-uppercase" href="/about">Tell Me More</a>
            </div>
        </header>

                <!-- Team-->
                <section class="page-section bg-light" id="team">
                    <div class="container">
                        <div class="text-center">
                            <h2 class="section-heading text-uppercase">Our Amazing Team</h2>
                            <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
                        </div>
                        <div class="row">
                            <% for (let i = 0; i < teams.length; i++) { %>
                            <div class="col-lg-4">
                                <div class="team-member">
                                    <img class="mx-auto rounded-circle" src="/assets/img/team/1.jpg" alt="..." />
                                    <h4><%= teams[i].firstName %> <%= teams[i].lastName %></h4>
                                    <p class="text-muted"><%= teams[i].job %></p>
                                    <a class="btn btn-dark btn-social mx-2" href="<%= teams[i].github %>"><i class="fab fa-github"></i></a>
                                    <a class="btn btn-dark btn-social mx-2" href="<%= teams[i].medium %>"><i class="fab fa-medium"></i></a>
                                    <a class="btn btn-dark btn-social mx-2" href="<%= teams[i].linkedin %>"><i class="fab fa-linkedin-in"></i></a>
                                </div>
                            </div>
                            <% } %>
                        </div>
                    </div>
                </section>

<!-- Footer-->
<%- include('partials/_footer') %>